beforeDevCommand = dotnet watch run --project src-blazor/src/{% project_name %}/{% project_name %}.csproj --no-launch-profile --urls http://localhost:1420
beforeBuildCommand = dotnet publish -c release src-blazor/src/{% project_name %}/{% project_name %}.csproj -o dist
devPath = http://localhost:1420
distDir = ../dist/wwwroot
withGlobalTauri = true